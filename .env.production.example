# Example production environment variables (do NOT commit secrets)
# Frontend: Vite will expose variables starting with VITE_
VITE_API_BASE=https://api.example.com

# Backend (set these securely in your host provider, not in VITE_ vars)
DB_HOST=your-db-host
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_NAME=workout_app
JWT_SECRET=a-very-strong-random-string
NODE_ENV=production
FRONTEND_URL=https://www.example.com
RAPIDAPI_KEY=your_rapidapi_key_here
EXERCISE_CACHE_TTL_SECONDS=86400
